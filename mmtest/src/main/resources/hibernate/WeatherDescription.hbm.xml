<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.mm.test.model.entity.WeatherDescription" table="WeatherDescription" catalog="cb_weatherinfo">
        <id name="DescriptionId" type="java.lang.int">
            <column name="DescriptionId" />
            <generator class="identity" />
        </id>
        <property name="WeatherDatetimeId" type="java.lang.int">
            <column name="WeatherDatetimeId" />
        </property>
        
        <property name="Main" type="java.lang.String">
            <column name="Main" length="20" />
        </property>
        <property name="Description" type="java.lang.String">
            <column name="Description" length="50" />
        </property>
       
    </class>
        
</hibernate-mapping>